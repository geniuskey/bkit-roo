{
  "name": "@bkit-roo/core",
  "version": "0.1.0",
  "description": "Integrated agent orchestrator",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bkit-roo/shared": "workspace:*",
    "@bkit-roo/api-client": "workspace:*",
    "@bkit-roo/parser": "workspace:*",
    "@bkit-roo/prompts": "workspace:*",
    "@bkit-roo/tools": "workspace:*",
    "@bkit-roo/modes": "workspace:*",
    "@bkit-roo/mcp-client": "workspace:*",
    "@bkit-roo/context": "workspace:*",
    "@bkit-roo/cost": "workspace:*",
    "@bkit-roo/file-rules": "workspace:*"
  }
}
